{
  "rewrites": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "(?<id>[a-z0-9]+)\\.lovefor\\.you"
        }
      ],
      "destination": "/v/:id"
    },
    {
      "source": "/((?!api|assets|sounds).*)",
      "destination": "/index.html"
    }
  ]
}
